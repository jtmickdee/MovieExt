{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension keeps track of what I watched online and blocks pop up",
  "version": "1.0",

  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
