{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension keeps track of what I watched online and blocks pop up",
  "version": "1.0",

  "content_scripts":[
    {
      "matches": ["https://*.solarmovie.is/*"],
      "js": ["js/cscript.js"]
    }
  ],

  "page_action": {
    "default_icon": "img/movie.png",
    "default_title": "Click here"
  },
  "permissions": [
    "tabs","https://*.solarmovie.is/*",
    "storage"
  ]
}
